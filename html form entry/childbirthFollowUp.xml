    <macro key="color" value="#e0e0ff" />

    <style>
        .section {
            border: 1px solid $headerColor;
            padding: 2px;
            text-align: left;
            margin-bottom: 1em;
        }

        .sectionHeader {
            background-color: $headerColor;
            color: $fontOnHeaderColor;
            display: block;
            padding: 2px;
            font-weight: bold;
            text-align: center;
        }

        table.baseline-aligned td {
            vertical-align: baseline;
        }

        h3 {
            text-decoration: underline;
        }

        table.baseline-aligned-under {
            border: 2px solid black;
        }

        table.baseline-aligned-under td {
            vertical-align: baseline;
        }

        table.baseline-aligned-borded td {
            vertical-align: baseline;
            border: 1px solid black;
        }
    </style>

    <ifMode mode="ENTER">
    <script type="text/javascript">
       
        var showHideNumber = function() {
            var cesarienneValue =$("cesarienne");
            console.log('-------enter val-------->>>' + cesarienneValue)
        };
       
        console.log('-------enter mode-------->>>')
        if (jQuery) {
            $j(document).ready(function () {
                getField('cesarienne.value').change(showHideNumber);
            });
        }
    </script>
</ifMode>

    <span style="float:right">Paper Form ID: $paperFormId</span>
    <h2>Suivi de l’accouchement (v1.0)</h2>
    <section headerLabel="ACCOUCHEMENT">
        <table class="baseline-aligned" border="1" width="100%">
            <tr>
                <td>Date d'admission:</td>
                <td colspan="3">
                    <obs conceptId="4447" />
                </td>
            </tr>
            <tr>
                <td>Debut de travail:</td>
                <td colspan="3">
                    <encounterDate default="today" showTime="true" />
                </td>
            </tr>
            <tr>
                <td>Prestataire:</td>
                <td colspan="3">
                    <encounterProviderAndRole default="currentUser" encounterRole="1" required="true" />
                </td>
            </tr>
            <tr>
                <td>Service:</td>
                <td colspan="3">
                    <encounterLocation required="true" tags="Admission Location,Transfer Location" />
                </td>
            </tr>


            <tr>
                <td>Motif à l’admission:</td>
                <td colspan="3">
                    <obs conceptId="382" size="120" />
                </td>
            </tr>
            <tr>
                <td>Suivi prénatal:</td>
                <td colspan="3">
                    <obs conceptId="$@is_suivi_prenatal@" answerConceptIds="$@YES@,$@NO@,$@Unknown@" answerLabels="Oui,Non,Inconnu" style="radio" />
                </td>
            </tr>
            <tr>
                <td>PTME:</td>
                <td colspan="3">
                    <obs conceptId="$@is_PMTE@" answerConceptIds="$@YES@,$@NO@,$@Unknown@" answerLabels="Oui,Non,Inconnu" style="radio" />
